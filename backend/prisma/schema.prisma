// This is your Prisma schema file
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model News {
  id          Int      @id @default(autoincrement())
  title       String?
  description String?
  content     String?
  url         String?
  image       String?
  publishedAt String?
  source      String?
  category    String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
